{
  "name": "back-server-911",
  "version": "1.0.0",
  "description": "servidor centralizado para apps de meniuz",
  "main": "index.js",
  "scripts": {
    "clean:modules": "rm -rf ./node_modules",
    "clean": "rm -rf ./build",
    "prepare": "husky install",
    "prebuild": "npm run clean",
    "build": "tsc",
    "cypress:open": "cypress open",
    "dev": "npm run clean && tsc-watch --onSuccess \"node build/src/app.js\"",
    "dev:ts": "ts-node-dev --respawn src/app.ts",
    "start": "npm run clean && tsc && node ./build/src/app.js",
    "format": "prettier --write \"src/**/*.ts\" --write \"package.json\"",
    "format:check": "prettier --check \"src/**/*.ts\" --check \"package.json\"",
    "lint": "eslint . --ext .js,.ts",
    "execute:insertBusiness": "ts-node-dev --respawn src/scripts/business.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GandresCoello18/meniuz-back.git"
  },
  "keywords": [
    "emergencia",
    "911"
  ],
  "author": "GandresCoello18",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GandresCoello18/meniuz-back/issues"
  },
  "homepage": "https://github.com/GandresCoello18/meniuz-back#readme",
  "dependencies": {
    "@semantic-release/changelog": "5.0.1",
    "@semantic-release/git": "9.0.0",
    "@sendgrid/mail": "^7.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/multer": "^1.4.7",
    "@types/mysql": "^2.15.17",
    "@types/node": "^18.8.3",
    "@types/node-cron": "^2.0.4",
    "@types/uuid": "^8.3.0",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^1.37.3",
    "cors": "^2.8.5",
    "date-fns": "^2.16.1",
    "email-validator": "^2.0.4",
    "envalid": "^6.0.2",
    "express": "^4.17.1",
    "express-rate-limit": "^5.5.1",
    "firebase-admin": "^11.1.0",
    "helmet": "^4.6.0",
    "jsonwebtoken": "^8.5.1",
    "mime": "^3.0.0",
    "multer": "^1.4.5-lts.1",
    "mysql": "^2.18.1",
    "node-color-log": "^7.0.0",
    "pino": "8.7.0",
    "typescript": "^4.8.4",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "8.3.5",
    "@commitlint/config-conventional": "8.3.4",
    "@typescript-eslint/eslint-plugin": "3.0.0",
    "@typescript-eslint/parser": "3.0.0",
    "commitizen": "4.1.2",
    "cz-conventional-changelog": "3.2.0",
    "env-cmd": "10.1.0",
    "eslint": "7.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-prettier": "3.1.3",
    "husky": "^8.0.1",
    "lint-staged": "10.2.6",
    "prettier": "2.0.5",
    "snyk": "^1.794.0",
    "ts-node-dev": "^2.0.0",
    "tsc-watch": "4.2.9"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts}": [
      "eslint"
    ]
  }
}
